{
  "name": "dojetest",
  "version": "1.0.0",
  "description": "도제학교 과정형평가 cbt",
  "main": "index.js",
  "scripts": {
    "dev": "cross-env NODE_ENV=development webpack-dev-server --open --hot",
    "build": "cross-env NODE_ENV=production webpack --progress --hide-modules",
    "start": "electron .",
    "watch": "webpack --watch --mode=development",
    "x64build": "electron-packager . lecshare --asar --platform=win32 --arch=x64 --out=dist64 --overwrite",
    "x32build": "electron-packager . lecshare --asar --platform=win32 --arch=ia32 --out=dist32 --overwrite"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gondr99/dojetest.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/gondr99/dojetest/issues"
  },
  "homepage": "https://github.com/gondr99/dojetest#readme",
  "devDependencies": {
    "asar": "^2.0.1",
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.5",
    "babel-preset-env": "^1.7.0",
    "babel-preset-stage-3": "^6.24.1",
    "css-loader": "^3.0.0",
    "electron": "^5.0.7",
    "electron-installer-squirrel-windows": "^1.3.0",
    "electron-packager": "^14.0.1",
    "file-loader": "^4.0.0",
    "vue-loader": "^15.7.0",
    "vue-router": "^3.0.7",
    "vue-template-compiler": "^2.6.10",
    "webpack": "^4.36.1",
    "webpack-cli": "^3.3.6"
  },
  "dependencies": {
    "express": "^4.17.1",
    "vue": "^2.6.10"
  }
}
